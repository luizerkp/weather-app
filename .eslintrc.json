{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": ["error"],
      "no-console": "off",
      "no-alert": "off",
      "quotes": [
        "error",
        "double"
      ],
      "wrap-iife": [
        "error", 
        "any"
      ],
      "no-param-reassign": [
        "error", 
        { 
          "props": false 
        }
      ]
    }
}
